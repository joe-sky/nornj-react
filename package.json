{
  "name": "nornj-react",
  "version": "5.0.0-alpha.2",
  "description": "React bindings for template engine NornJ",
  "main": "lib/base.js",
  "files": [
    "dist/",
    "lib/",
    "mobx/",
    "native/",
    "redux/",
    "router/",
    "server/",
    "src/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "umd": "cross-env BABEL_ENV=webpack webpack",
    "umd:mobx": "cross-env BABEL_ENV=webpack WITH_EX=mobx webpack",
    "umd:redux": "cross-env BABEL_ENV=webpack WITH_EX=redux webpack",
    "umd:router": "cross-env BABEL_ENV=webpack WITH_EX=router webpack",
    "min": "cross-env BABEL_ENV=webpack NODE_ENV=production webpack",
    "min:mobx": "cross-env BABEL_ENV=webpack NODE_ENV=production WITH_EX=mobx webpack",
    "min:redux": "cross-env BABEL_ENV=webpack NODE_ENV=production WITH_EX=redux webpack",
    "min:router": "cross-env BABEL_ENV=webpack NODE_ENV=production WITH_EX=router webpack",
    "lib": "babel src --out-dir lib",
    "lib:mobx": "babel mobx/base.js --out-file mobx/index.js && babel mobx/baseNative.js --out-file mobx/native.js && babel mobx/src --out-dir mobx/lib",
    "lib:redux": "babel redux/base.js --out-file redux/index.js ",
    "lib:router": "babel router/base.js --out-file router/index.js ",
    "lib:native": "babel native/base.js --out-file native/index.js ",
    "lib:server": "babel server/base.js --out-file server/index.js ",
    "build": "npm run lib && npm run lib:mobx && npm run lib:redux && npm run lib:router && npm run lib:native && npm run lib:server && npm run umd && npm run umd:mobx && npm run umd:redux && npm run umd:router && npm run min && npm run min:mobx && npm run min:redux && npm run min:router",
    "lint": "eslint src"
  },
  "keywords": [
    "nornj",
    "nj",
    "template",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/joe-sky/nornj-react.git"
  },
  "author": "Joe_Sky <hitomi_doax@sina.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.22.0",
    "cross-env": "^5.1.0",
    "eslint": "^4.9.0",
    "gulp": "^3.9.1",
    "gulp-jasmine": "^2.1.0",
    "gulp-notify": "^2.2.0",
    "jsdom": "^8.1.0",
    "lodash": "^4.13.1",
    "nornj": "^5.0.0-alpha.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "webpack": "^3.8.1",
    "yargs": "^4.2.0"
  }
}
