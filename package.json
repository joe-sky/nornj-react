{
  "name": "nornj-react",
  "version": "5.0.0-rc.9",
  "description": "React bindings for NornJ template engine.",
  "main": "dist/nornj-react.common.js",
  "module": "dist/nornj-react.esm.js",
  "unpkg": "dist/nornj-react.js",
  "jsdelivr": "dist/nornj-react.js",
  "files": [
    "dist/",
    "lib/",
    "mobx/",
    "native/",
    "redux/",
    "router/",
    "antd/",
    "echarts/",
    "src/",
    "index.d.ts",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "cjs": "cross-env NODE_ENV=cjs rollup -c -o dist/nornj-react.common.js",
    "esm": "cross-env NODE_ENV=es rollup -c -o dist/nornj-react.esm.js",
    "umd": "cross-env NODE_ENV=development rollup -c -o dist/nornj-react.js",
    "min": "cross-env NODE_ENV=production rollup -c -o dist/nornj-react.min.js",
    "umd:mobx": "cross-env NODE_ENV=development WITH_EX=Mobx rollup -c -o dist/nornj-react-mobx.js",
    "min:mobx": "cross-env NODE_ENV=production WITH_EX=Mobx rollup -c -o dist/nornj-react-mobx.min.js",
    "umd:redux": "cross-env NODE_ENV=development WITH_EX=Redux rollup -c -o dist/nornj-react-redux.js",
    "min:redux": "cross-env NODE_ENV=production WITH_EX=Redux rollup -c -o dist/nornj-react-redux.min.js",
    "umd:router": "cross-env NODE_ENV=development WITH_EX=Router rollup -c -o dist/nornj-react-router.js",
    "min:router": "cross-env NODE_ENV=production WITH_EX=Router rollup -c -o dist/nornj-react-router.min.js",
    "lib": "babel src --out-dir lib",
    "lib:mobx": "babel mobx/base.js --out-file mobx/index.js && babel mobx/baseNative.js --out-file mobx/native.js && babel mobx/src --out-dir mobx/lib",
    "lib:redux": "babel redux/base.js --out-file redux/index.js ",
    "lib:router": "babel router/base.js --out-file router/index.js ",
    "lib:native": "babel native/base.js --out-file native/index.js ",
    "lib:antd": "babel antd --out-dir antd/lib --ignore \"antd/lib\"",
    "lib:echarts": "babel echarts/base.js --out-file echarts/index.js && babel echarts/baseCore.js --out-file echarts/core.js",
    "build": "npm run lib && npm run lib:mobx && npm run lib:redux && npm run lib:router && npm run lib:native && npm run lib:antd && npm run lib:echarts && npm run cjs && npm run esm && npm run umd && npm run umd:mobx && npm run umd:redux && npm run umd:router && npm run min && npm run min:mobx && npm run min:redux && npm run min:router",
    "lint": "eslint src",
    "test": "jest --config=test/jest/jest.config.json",
    "coverage": "jest --coverage --config=test/jest/jest.config.json"
  },
  "keywords": [
    "nornj",
    "nj",
    "template",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/joe-sky/nornj-react.git"
  },
  "author": "Joe_Sky <hitomi_doax@sina.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "antd": "^3.8.1",
    "babel-eslint": "^8.0.1",
    "babel-plugin-nornj-in-jsx": "^5.0.0-rc.9",
    "cross-env": "^5.1.0",
    "echarts-for-react": "^2.0.15-beta.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^4.9.0",
    "jest": "^24.3.1",
    "jest-css-modules-transform": "^2.1.1",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "lodash": "^4.13.1",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "mobx-react-lite": "^1.2.0",
    "nornj": "^5.0.0-rc.7",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "rollup": "^1.4.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-license": "^0.8.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.2"
  }
}
